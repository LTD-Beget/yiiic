{
    "name": "ltd-beget/yiiic",
    "description": "yii2 interactive console with smart autocomplete and context workflow",
    "type": "yii2-extension",
    "license": "MIT",
    "keywords": ["yii2", "console", "interactive", "shell"],
    "authors": [
        {
            "name": "Danila Stivrinsh",
            "email": "danilaridzhi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0",
        "symfony/process": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "LTDBeget\\Yiiic\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LTDBeget\\Yiiic\\Tests\\": [
                "tests/"
            ]
        }
    },
    "min-stability": "dev"
}
